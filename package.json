{
  "name": "imsam-ai-interview",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "cap:sync": "npx cap sync",
    "cap:ios": "npx cap open ios",
    "cap:dev": "CAP_DEV=true npx cap run ios",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:link": "supabase link --project-ref whczodrinaeeemwncfze",
    "db:push": "supabase db push --linked",
    "db:pull": "supabase db pull --linked",
    "db:seed": "echo '시드 데이터는 Supabase Dashboard > SQL Editor에서 supabase/seed.sql 내용을 실행하세요'",
    "db:migrate:new": "supabase migration new",
    "db:migrate:list": "supabase migration list --linked",
    "db:reset": "supabase db reset --linked",
    "db:types": "supabase gen types typescript --linked > types/supabase.ts",
    "functions:serve": "supabase functions serve --env-file .env.local",
    "functions:deploy": "supabase functions deploy --linked",
    "functions:deploy:auto-score": "supabase functions deploy auto-score --linked",
    "functions:deploy:generate-report": "supabase functions deploy generate-report --linked",
    "functions:deploy:update-rankings": "supabase functions deploy update-rankings --linked",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:all": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "version:patch": "npm version patch -m 'chore(release): %s'",
    "version:minor": "npm version minor -m 'chore(release): %s'",
    "version:major": "npm version major -m 'chore(release): %s'"
