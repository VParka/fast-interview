{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "asia-east1"
    }
  },
  "functions": {
    "source": ".firebase/functions"
  },
  "rewrites": [
    {
      "source": "**",
      "function": "nextjs16app"
    }
  ],
  "emulators": {
    "hosting": {
      "port": 5000
    }
  }
}
